{
    "name" : "Holy Quran",
    "version" : "1.0.0",
    "manifest_version" : 2,
    "description" : "The Holy Quran with English Translation.",
    "icons" : {
        "16" : "img/icon.png",
        "48" : "img/48.png",
        "128" : "img/128.png"
    },
    "homepage_url" : "https://codeinphp.github.io/",
    "background":{
        "scripts":["js/jquery.js"]
    },
    "permissions" : [
        "tabs",
        "<all_urls>"
    ],
    "browser_action" : {
        "default_icon" : "img/icon.png",
        "default_title" : "Holy Quran",
        "default_popup" : "popup.html"
    }
}